name = Bootstrap Starter
description = A Bootstrap Starter Theme.
core = 7.x
base theme = bootstrap_ignitor


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[navigation]     = 'Navigation'
regions[header]         = 'Top Bar'
regions[highlighted]    = 'Highlighted'
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[sidebar_first]  = 'Primary'
regions[sidebar_second] = 'Secondary'
regions[footer]         = 'Footer'
regions[page_top]       = 'Page top'
regions[page_bottom]    = 'Page bottom'


;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/base.css
stylesheets[all][] = css/layout.css
stylesheets[all][] = css/component.css
stylesheets[all][] = css/modifiers.css
;; Remove if you want to include jScrollPane (a scrollbar replacement)
; stylesheets[all][] = css/vendor/jquery.jscrollpane.css

;;;;;;;;;;;;;;;;;;;;;
;; Scripts
;;;;;;;;;;;;;;;;;;;;;

scripts[] = 'js/vendor/respond.min.js'
scripts[] = 'js/vendor/modernizr.custom.js'
scripts[] = 'js/vendor/jquery.touchSwipe.min.js'
scripts[] = 'js/vendor/jquery.mousewheel.min.js'
;; Remove if you want to include jScrollPane (a scrollbar replacement)
; scripts[] = 'js/vendor/jquery.jscrollpane.min.js'
